<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springbootdemo.mybatis.mapper.TbOrderMapper">

    <select id="getPageListByQuery" resultType="com.example.springbootdemo.mybatis.entity.TbOrder">
        select id,
               user_id,
               product_id,
               count,
               money,
               status
        from tb_order

    </select>
</mapper>
